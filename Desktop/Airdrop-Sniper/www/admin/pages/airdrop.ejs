<%- include('../partials/_header');  %>

		<!-- partial:../../partials/_sidebar.html -->
	<%- include('../partials/_sidebar')  %>
		<!-- partial -->
	
		<div class="page-wrapper">
			<%- include('../partials/_navbar')  %>
			<!-- partial -->

			<div class="page-content">

				<nav class="page-breadcrumb">
					<ol class="breadcrumb">
						<li class="breadcrumb-item"><a href="#">Site</a></li>
						<li class="breadcrumb-item active" aria-current="page">Airdrop</li>
					</ol>
				</nav>

				<div class="row">
					<div class="col-md-12 grid-margin">
						<div class="card">
							<div class="card-body">
								<h6 class="card-title">Airdrops</h6>
								<form method="POST" id="airdrops"  class="forms-sample" > 
									<div class="row mb-3">
										<div class="col">
											<label class="form-label">Başlık  :</label>
											<input class="form-control mb-4 mb-md-0" name="baslik" id="baslik" value="<%=  airdrop.baslik %>"/>
										</div>
										<div class="col">
											<label class="form-label">Sarı Başlık :</label>
											<input class="form-control mb-4 mb-md-0" name="saribaslik" id="saribaslik" value="<%= airdrop.saribaslik %>"/>
										</div>
										<div class="col">
											<label class="form-label">Alt Başlık:</label>
											<input class="form-control mb-4 mb-md-0" name="altbaslik" id="altbaslik" value="<%= airdrop.altbaslik %>"/>
										</div>
										<div class="col">
											<label class="form-label">Buton Adı</label>
											<input class="form-control mb-4 mb-md-0" name="butonadi" id="butonadi" value="<%=   airdrop.butonadi %>"/>
										</div>
										<div class="col">
											<label class="form-label">Başlangıç Yazısı :</label>
											<input class="form-control mb-4 mb-md-0" name="baslangic" id="baslangic" value="<%=  airdrop.baslangic %>"/>
										</div>
									</div>           
									<div class="row mb-3">
										<div class="col">
											<label class="form-label">Bitiş Yazısı:</label>
											<input class="form-control mb-4 mb-md-0" name="bitis" id="bitis" value="<%=  airdrop.bitis  %>"/>
										</div>
											<div class="col">
											<label class="form-label">Url :</label>
											<input class="form-control mb-4 mb-md-0" name="url" id="url" value="<%=  airdrop.url %>"/>
										</div>
										
									</div>           
								
                                        <button type="submit" class="btn btn-primary">Güncelle</button>
								</form>
						</div>
					</div>
				</div>
				
			
				</body>
                          
			<%- include('../partials/_footer');  %>
		</div>
	</div>
 

