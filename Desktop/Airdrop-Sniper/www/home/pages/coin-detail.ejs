  <script>
    const copyContent = document.querySelector('.copy-content');
    const addressText = copyContent.querySelector('.address-text');

    copyContent.addEventListener('click', () => {
        const copyText = copyContent.getAttribute('data-copytext');
        copyToClipboard(copyText);
        addressText.innerText = 'Copied!';
        setTimeout(() => {
            addressText.innerText = copyText.slice(0, 6) + '...' + copyText.slice(-4);
        }, 1000); // 1 saniye sonra özetlenmiş adres geri gelir
    });

    function copyToClipboard(text) {
        const textarea = document.createElement('textarea');
        textarea.value = text;
        document.body.appendChild(textarea);
        textarea.select();
        document.execCommand('copy');
        document.body.removeChild(textarea);
    }
</script>
